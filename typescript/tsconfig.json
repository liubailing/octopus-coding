
{
	"compilerOptions": {
		"declaration": true,
		"importHelpers": true,
		"module": "esnext",
		"outDir": "lib",
		"rootDir": "src",
		"strict": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react",
		"target": "es2017"
	},
	"include": ["src/**/*"]
}
