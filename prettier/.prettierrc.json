{
	"useTabs": true,
	"tabWidth": 4,
	"endOfLine": "lf",
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"eslintIntegration": false,
	"tslintIntegration": false,
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"disableLanguages": ["vue"],
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"ignorePath": ".prettierignore"
}
